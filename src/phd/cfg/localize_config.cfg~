#!/usr/bin/env python
PACKAGE = "phd"

from dynamic_reconfigure.parameter_generator_catkin import *

gen = ParameterGenerator()

gen.add("accuracy",    double_t,    0, "Marker Point Accuracy", 0.01, 0, 1)
gen.add("intensity_min",    int_t,    0, "Marker Intensiy Cutoff", 700, 0, 3000)
gen.add("intensity_max",    int_t,    0, "Marker Intensiy Cutoff", 1200, 0, 3000)


exit(gen.generate(PACKAGE, "localization_service", "Localize"))



